<%- include("header") -%>

<div class="box" id="heading">
    <h1>Your note pages</h1>
  </div>
  <div class="box" id="heading">
    <h1>Home</h1>
  </div>

  <div class="box">
    <% newListItems.forEach(function(item){ %>
      <form action="/open" method="post">
        <button style="background-color: white; width: 100%; height: 100%; cursor: pointer;" type="submit" value="<%= item.name %>" name="ad">
            <div class="item">
              Open
              <p><%= item.name %></p>
            </div>
        </button>
        
      </form>
      <% }) %>

      <form class="item" action="/doc/newdoc" method="post">
        <input type="text" name="newdoc" placeholder="New Item" autocomplete="off">
        <button type="submit" name="list">+</button>
      </form>

  </div>

<%- include("footer") -%>
